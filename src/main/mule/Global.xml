	<http:request-config name="HTTP_Request_configuration" doc:name="HTTP Request configuration" doc:id="b6741487-c6a4-4240-863a-f4eb01fe84b7" basePath="${httpRequest.basePath}" >
		<http:request-connection host="${httpRequest.host}"/>
	</http:request-config>
