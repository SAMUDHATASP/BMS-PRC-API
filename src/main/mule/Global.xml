		<http:listener-connection host="${http.host}" port="${http.port}" />
	</http:listener-config>
	<api-gateway:autodiscovery apiId="${autoDiscovery.Id}" ignoreBasePath="true" doc:name="API Autodiscovery" doc:id="e2932697-2e2f-4cc9-a00e-bbce5d7021db" flowRef="${autoDiscovery.flowName}" />

	
	
	
	</mule>
